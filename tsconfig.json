{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@/utils/*": [
        "utils/*"
      ]
      // Add more aliases as needed
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/components/Checkout",
    "src/app/components/sidebar/sidebar.jsx",
    "src/app/components/Layout.jsx",
    "src/app/components/Sidebar.jsx",
    "src/app/admin/components/Navbar.js",
    "src/app/helpers/. validateTextField tsx"
, "src/app/admin/page.tsx", "src/components/Profile", "src/components/Profile"  ],
  "exclude": [
    "node_modules"
  ]
}